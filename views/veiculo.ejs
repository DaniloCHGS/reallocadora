    <%-include('partials/_head.ejs')%>
    <body class="loading" data-layout='{"mode": "light", "width": "fluid", "menuPosition": "fixed", "sidebar": { "color": "light", "size": "default", "showuser": false}, "topbar": {"color": "dark"}, "showRightSidebarOnPageLoad": true}'>
        
        <!-- Begin page -->
        <div id="wrapper">

            <!-- Topbar Start -->
            <%-include('partials/_nav.ejs')  %>
            <!-- end Topbar -->

            <!-- ========== Left Sidebar Start ========== -->
            <%-include('partials/_sideBar.ejs')%>
            <!-- Left Sidebar End -->

            <!-- ============================================================== -->
            <!-- Start Page Content here -->
            <!-- ============================================================== -->

            <div class="content-page" id="veiculo">
                <div class="content">

                    <!-- Start Content-->
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-12">
                                <div class="page-title-box">
                                    <div class="page-title-right">

                                        <a class="btn btn-success" href="/painel/veiculos">Voltar</a>

                                        <a class="btn btn-warning" href="/painel/veiculo/editar/<%=veiculo.id%>">Editar</a>

                                        <button type="button" class="btn btn-danger waves-effect waves-light" data-toggle="modal" data-target="#custom-modal">
                                           Excluir
                                        </button>

                                    </div>
                                    <h4 class="page-title">Veículo</h4>
                                    <%-include('partials/_msg-sucesso.ejs')%>
                                </div>
                            </div>
                        </div> 
                        
                        <div class="row">
                            <div class="col-xl-12">
                                <div class="card">
                                    <div class="card-body">
                                        
                                        <div class="row">
                                            <div class="col-lg-2">
                                                <h4 class="page-title">Nome do Fornecedor</h4>
                                                <p><%=veiculo.nomeFornecedor%></p>
                                            </div>
                                            <div class="col-lg-2">
                                                <h4 class="page-title">Renavan</h4>
                                                <p><%=veiculo.renavan%></p>
                                            </div>
                                            
                                            <div class="col-lg-2">
                                                <h4 class="page-title">Tipo</h4>
                                                <p><%=veiculo.tipo%></p>
                                            </div>
                                            <div class="col-lg-2">
                                                <h4 class="page-title">Valor de Compra</h4>
                                                <p><%=veiculo.valorCompra%></p>
                                            </div>
                                            <div class="col-lg-2">
                                                <h4 class="page-title">Valor de Venda</h4>
                                                <p><%=veiculo.valorVenda%></p>
                                            </div>
                                            <div class="col-lg-2">
                                                <h4 class="page-title">KM</h4>
                                                <p>2000</p>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-2">
                                                <h4 class="page-title">Placa</h4>
                                                <p><%=veiculo.placa%></p>
                                            </div>
                                            <div class="col-lg-2">
                                                <h4 class="page-title">Marca</h4>
                                                <p><%=veiculo.marca%></p>
                                            </div>
                                            <div class="col-lg-2">
                                                <h4 class="page-title">Ano</h4>
                                                <p><%=veiculo.ano%></p>
                                            </div>
                                            <div class="col-lg-2">
                                                <h4 class="page-title">Modelo</h4>
                                                <p><%=veiculo.modelo%></p>
                                            </div>
                                            <div class="col-lg-2">
                                                <h4 class="page-title">Status</h4>
                                                <p class="badge badge-success"><%=veiculo.status%></p>
                                            </div>
                                        </div>
                                    </div> <!-- end card-body-->
                                </div> <!-- end card-->
                            </div>
                            <!-- end col-->
                        </div>

                        <!--primeira tabela -->

                        <div class="row">
                            <div class="col-12">
                                <div class="page-title-box">
                                    <h4 class="page-title">Manutenção</h4>
                                </div>
                                <div class="card">
                                    <div class="card-body">
                                        <table id="" class="table dt-responsive nowrap w-100 table-striped basic-datatable">
                                            <thead>
                                                <tr>
                                                    <th>Data</th>
                                                    <th>Km</th>
                                                    <th>Descrição</th>
                                                    <th>Status</th>
                                                    <th>Informações</th>
                                                </tr>
                                            </thead>
                                        
                                        
                                            <tbody>
                                                <% manutencoes.forEach(manutencao=>{ %>
                                                <tr>
                                                    <%if(manutencao.placa == veiculo.placa) {%>
                                                        <td><%= manutencao.data %></td>
                                                        <td><%= manutencao.km %></td>
                                                        <td><%= manutencao.desc %></td>
                                                        <td>
                                                            <p><%= (manutencao.fechamento) ? 'Finalizada: '+manutencao.fechamento : 'Em aberto' %></p>
                                                        </td>
                                                        <td>
                                                            <a href="/painel/manutencao/gasto/<%=manutencao.id%>" target="_blank" class="btn btn-info">Ver mais</a>
                                                        </td>
                                                    <%}%>
                                                </tr>
                                                <% }) %>
                                            </tbody>
                                        </table>

                                    </div> <!-- end card body-->
                                </div> <!-- end card -->
                            </div><!-- end col-->
                        </div>

                        <!--Segunda tabela -->

                        <div class="row">
                            <div class="col-12">
                                <div class="page-title-box">
                                    <h4 class="page-title">Contratos</h4>
                                </div>
                                <div class="card">
                                    <div class="card-body">
                                        <table id="" class="table dt-responsive nowrap w-100 table-striped basic-datatable">
                                            <thead>
                                                <tr>
                                                    <th>Contrato</th>
                                                    <th>Ano</th>
                                                    <th>Orgão</th>
                                                    <th>Informações</th>
                                                </tr>
                                            </thead>

                                            <tbody>
                                                <%contratos.forEach(contrato=>{ %>
                                                    <%ids.forEach(id=>{ %>
                                                        <%if(contrato.id == id) {%>
                                                            <tr>
                                                                <td><%=contrato.numero%></td>
                                                                <td><%=contrato.inicio%></td>
                                                                <td><%=contrato.orgao%></td>
            
                                                                <td>
                                                                    <a href="/painel/contrato/<%=contrato.id%>" class="btn btn-info" target="_blank">Ver mais</a>
                                                                </td>
                                                            </tr>
                                                        <% } %>
                                                    <% }) %>
                                                
                                                <% }) %>
                                            </tbody>
                                        </table>

                                    </div> <!-- end card body-->
                                </div> <!-- end card -->
                            </div><!-- end col-->
                        </div>

                    </div>
                    <!-- container -->

                </div> <!-- content -->

                <%-include('partials/_footer.ejs')%>

            </div>

            <!-- ============================================================== -->
            <!-- End Page content -->
            <!-- ============================================================== -->


        </div>
        <!-- END wrapper -->

        <!-- Vendor js -->
        <script src="/assets/js/vendor.min.js"></script>

        <!-- Plugins js-->
        <script src="/assets/libs/flatpickr/flatpickr.min.js"></script>
        <!-- <script src="/assets/libs/apexcharts/apexcharts.min.js"></script> -->

        <script src="/assets/libs/selectize/js/standalone/selectize.min.js"></script>

        <%-include('partials/_script_dataTable.ejs')%>
        <!-- third party js ends -->

        <!-- App js-->
        <script src="/assets/js/app.min.js"></script>

        <div class="modal fade" id="custom-modal" tabindex="-1" role="dialog" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered">
                <div class="modal-content">
                    <div class="modal-header bg-light">
                        <h4 class="modal-title" id="myCenterModalLabel">Deseja excluir?</h4>
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    </div>
                    <div class="modal-body p-4">
                        <a href="/painel/veiculo/excluir/<%=veiculo.id%>" class="btn btn-block btn-danger">Sim, excluir!</a>
                    </div>
                </div><!-- /.modal-content -->
            </div><!-- /.modal-dialog -->
        </div><!-- /.modal -->
        
    </body>
</html>